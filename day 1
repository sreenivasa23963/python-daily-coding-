n = list(map(int,input("enter").split()))
positive = 0
negative = 0
for i in range(0,len(n)):
    if n[i] < 0:
        positive += 1
    else:
        negative += 1
print("number of positive numbers",positive)
print("number of negative numbers",negative)

n = list(map(int,input("Enter numbers to find sum of even number : ").split()))
even = 0
odd = 0
for i in range(0,len(n)):
    if n[i] % 2 == 0:
        even += n[i]
    else:
        odd += n[i]
print(even-odd)

n = list(map(int,input("Enter numbers : ").split()))
for i in range(0,len(n)):
    if i % 2 == 0 and n[i] % 2 == 0:
        print(n[i])

s = input("Enter a string ")
r = ''
for i in range(len(s)-1,-1,-1):
    r += s[i]
if s == r:
    print('palindrome')
else:
    print('not palindrome')

s = input("Enter a string ")
vowel ='a', 'e', 'i', 'o', 'u'
count = 0
for i in range(0,len(s)):
    if s[i] in vowel:
        count += 1
print(count)

s = input("Enter a string ")
count = 0
vowels = 'a', 'e', 'i', 'o', 'u'
for i in range(0,len(s)):
    if s[i] not in vowels:
        count += 1
print(count, 'consonants')

s = input("Enter a string ")
count = 0
for i in range(len(s)):
    if '0' <= s[i] and s[i] <= '9':
        count += 1
print(count)

s = input("Enter a string ")
at_count=0
dot_count=0
for i in range(len(s)):
    if s[i] == '@':
        at_count += 1
    elif s[i] == '.':
        dot_count += 1
if at_count == 1 and dot_count == 1:
    print('valid Gmail')
else:
    print('not valid Gmail')

s = input("Enter a string ")
count = 0
for i in range(len(s)):
    if s[i] == ' ':
        count += 1
print(count+1)

