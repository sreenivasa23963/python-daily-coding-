n = list(map(int,input("Enter numbers to find biggest even number from the list : ").split()))
even_count = []
for i in range(0,len(n)):
    if n[i] % 2 == 0:
        even_count.append(n[i])
print("Even number list",even_count)
big = even_count[0]
for j in range(1,len(even_count)):
    if even_count[j] > big:
        big = even_count[j]
print(big)

